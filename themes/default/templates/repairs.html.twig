{# the sales page. Should contain an information block and panels with car info linking to individual car info #}
{% extends 'base.html.twig' %}
{% set active_page = 'repairs' %}
{% block title %}
    {{ active_page }}
{% endblock %}
{% block unique_page_content %}
    {% for car in result %}
        <div class="uk-width-medium-1-3">
            <div class="uk-panel uk-panel-box uk-panel-header">
                <h2 class="uk-panel-title">Repairs</h2>



                <p>{{ car.make }}</p>
                <p>{{ car.model }}</p>


                <a href="#" class="uk-button uk-button-primary">More Details</a>
            </div>
        </div>
    {% endfor %}
{% endblock %}
{% block content %}
{% endblock %}